Dim imie As String
Dim nazwisko As String
Dim wzrost As Integer
Dim wiek As Integer

Range("A1") = InputBox("Podaj imie")
Range("A2") = InputBox("Podaj nazwisko")
Range("A3") = InputBox("Podaj wzrost")
Range("A4") = InputBox("Podaj wiek")

imie = Range("A1").Value
nazwisko = Range("A2").Value
wzrost = Range("A3").Value
wiek = Range("A4").Value

Range("B1") = imie
Range("B2") = nazwisko
Range("B3") = wzrost
Range("B4") = wiek
